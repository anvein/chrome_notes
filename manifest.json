{
    "manifest_version": 2,

    "name": "Chrome Notes",
    "version": "1.0",

    "icons": {
        "16": "icons/16x16.png",
        "32": "icons/32x32.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png"
    },
    "browser_action": {
        "default_title": "Chrome Notes",
        "default_icon": "icons/48x48.png",
        "default_popup": "screen.html"
    },
    "permissions": [
        "storage"
    ]
}